  "description": "Backend service for the GitSec application",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server.ts",
    "build": "cross-env NODE_ENV=production tsc",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint \"**/*.{js,ts,mjs}\"",
    "lint:fix": "eslint \"**/*.{js,ts,mjs}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,mjs,json,md}\"",
    "typecheck": "tsc --noEmit"
  },
